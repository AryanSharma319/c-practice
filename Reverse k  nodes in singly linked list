#include<stdio.h>
#include<stdlib.h>

struct Node
{
    int data;
    struct Node *next;
};

struct Node *Creator(struct Node *head, int n)
{
    struct Node *temp=head;
    for(int i=0;i<n;i++)
    {
        struct Node *newnode=malloc(sizeof(struct Node));
        newnode->next=NULL;
        printf("Enter Data:");
        scanf("%d",&newnode->data);
        if(head==NULL)
        {
            head=newnode;
            temp=head;
        }
        else
        {
            newnode->next=NULL;
            temp->next=newnode;
            temp=temp->next;
        }
    }
    return head;
}

void Displayer(struct Node *head)
{
    struct Node *temp=head;
    while(temp!=NULL)
    {
        printf("%d->",temp->data);
        temp=temp->next;
    }
    printf("NULL");
}

struct Node *Rotater(struct Node *head, int k)
{
    struct Node *temp=head, *prev=NULL, *next=NULL;
    int count=0;
    while(temp!=NULL && count<k)
    {
        next=temp->next;
        temp->next=prev;
        prev=temp;
        temp=next;
        count++;
    }
    if(next!=NULL)
    {
        head->next=Rotater(next,k);
    }
    return prev;
}

void main(void)
{
    int n;
    printf("Enter Nodes:");
    scanf("%d",&n);
    struct Node *head=NULL;
    head=Creator(head,n);
    Displayer(head);
    printf("\nEnter k:");
    int k;
    scanf("%d",&k);
    head=Rotater(head,k);
    Displayer(head);
}
